
#section-contact-module {
    padding-top: 44px;
    background-size: cover !important;
    background-repeat: no-repeat;
    background-position: center !important;
    margin-bottom: 74px;

    @media screen and (min-width: desktop-min) {
        background: url('../../imgs/banner/bg-contact-module.jpg');
        height: 235px;
    }

    @media screen and (min-width: mobile-min) and (max-width: tablet-max) {
        background: url('../../imgs/banner/bg-contact-module-mob.jpg');
        height: 269px;
        padding-top: 23px;
        margin-bottom: 45px;
    }

    .title-large {
        color: branco;
        text-shadow: 0px 3px 11px #000;
    }

    .title-before {
        margin-top: 25px;
    }

    .button {
        margin-top: 28px;
    }
}